{
  "name": "node-app",
  "version": "0.1.0",
  "scripts": {
    "dev": "nodemon --exec babel-node ./src/bin/www",
    "start": "babel-node ./src/bin/www",
    "transpile": "babel ./src/bin --out-dir dist-server",
    "pm2": "pm2",
    "pm2:start": "pm2 start ./src/bin/www",
    "pm2:stop": "pm2 stop ./src/bin/www",
    "pm2:restart": "pm2 restart ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.6",
    "debug": "~4.3.4",
    "express": "~4.18.2",
    "http-errors": "~2.0.0",
    "jade": "~1.11.0",
    "morgan": "~1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/node": "^7.23.9",
    "@babel/plugin-transform-optional-chaining": "^7.23.4",
    "@babel/preset-env": "^7.23.9",
    "@babel/register": "^7.23.7",
    "@babel/runtime": "^7.23.9",
    "nodemon": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "pm2": "^5.3.1",
    "rimraf": "^5.0.5"
  }
}
